{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "shippedProposals": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "corejs": 3,
        "version": "^7.20.13"
      }
    ],
    [
      "lodash"
    ]
  ],
  "sourceType": "unambiguous",
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "shippedProposals": true
          }
        ]
      ],
      "plugins": [
        "transform-require-context"
      ]
    }
  }
}
