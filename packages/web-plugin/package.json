{
  "name": "@mpxjs/web-plugin",
  "version": "2.7.40",
  "description": "mpx compile core",
  "keywords": [
    "mpx"
  ],
  "homepage": "https://didi.github.io/mpx/",
  "bugs": {
    "url": "https://github.com/didi/mpx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:didi/mpx.git"
  },
  "license": "Apache",
  "author": "donghongping",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./index": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./*": "./*",
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist",
    "src/runtime"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc && npm run post-build",
    "dev": "node scripts/tsc.js",
    "post-build": "node scripts/postbuild.js executive"
  },
  "dependencies": {
    "@ampproject/remapping": "^2.2.0",
    "@babel/generator": "^7.16.0",
    "@babel/types": "^7.16.0",
    "@better-scroll/core": "^2.5.0",
    "@better-scroll/movable": "^2.5.0",
    "@better-scroll/observe-dom": "^2.5.0",
    "@better-scroll/pull-down": "^2.5.0",
    "@better-scroll/slide": "^2.5.0",
    "@better-scroll/wheel": "^2.5.0",
    "@better-scroll/zoom": "^2.5.0",
    "@mpxjs/compile-utils": "workspace:*",
    "@mpxjs/compiler": "workspace:*",
    "@mpxjs/loaders": "workspace:*",
    "@mpxjs/plugin-proxy": "workspace:*",
    "@vitejs/plugin-vue2": "^2.0.0",
    "async": "^2.6.0",
    "debug": "^4.3.2",
    "hash-sum": "^1.0.2",
    "json5": "^2.1.3",
    "loader-utils": "^2.0.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.15",
    "lru-cache": "^4.1.2",
    "magic-string": "^0.27.0",
    "mime": "^2.2.2",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "qs": "^6.10.1",
    "slash": "^3.0.0",
    "source-list-map": "^2.0.0"
  },
  "devDependencies": {
    "@mpxjs/webpack-plugin": "workspace:*",
    "esbuild": "^0.16.17",
    "fast-glob": "^3.2.12",
    "rollup": "^3.0.0",
    "vue": "^2.7.0",
    "vite": "^4.0.0",
    "webpack": "^5.48.0"
  },
  "peerDependencies": {
    "@mpxjs/webpack-plugin": "2.8.20",
    "vue": "^2.7.0"
  },
  "engines": {
    "node": ">=14.14.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
