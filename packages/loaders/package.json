{
  "name": "@mpxjs/loaders",
  "version": "2.7.40",
  "description": "mpx loaders",
  "keywords": [
    "mpx"
  ],
  "author": "lareinayanyu",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc -w",
    "build": "rimraf ./dist && tsc"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "require": "./dist/*",
      "import": "./dist/*"
    }
  },
  "dependencies": {
    "@babel/generator": "^7.16.0",
    "@babel/parser": "^7.16.2",
    "@babel/traverse": "^7.16.0",
    "@babel/types": "^7.16.0",
    "@mpxjs/compile-utils": "workspace:*",
    "@mpxjs/compiler":  "workspace:*",
    "@mpxjs/plugin-proxy": "workspace:*",
    "loader-utils": "^2.0.2",
    "mime": "^2.6.0"
  },
  "devDependencies": {
    "@mpxjs/webpack-plugin": "workspace:*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:didi/mpx.git"
  },
  "homepage": "https://didi.github.io/mpx/",
  "bugs": {
    "url": "https://github.com/didi/mpx/issues"
  },
  "peerDependencies": {
    "@mpxjs/webpack-plugin": "2.8.20"
  }
}
